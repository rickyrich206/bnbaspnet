<%@ Control Language="C#" AutoEventWireup="true" CodeBehind="PigDetail.ascx.cs" Inherits="PMS.UserControl.PigDetail" %>
<div class="row">
    <div class="col-lg-12">
        <hr style="width: 99%; color: black; height: 1px; background-color: black;" />
